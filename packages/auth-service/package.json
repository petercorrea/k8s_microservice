{
  "name": "auth-service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "pre-commit": "npm run lint && npm run type-check && npm tun test",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "type-check": "tsc --noEmit",
    "gen-schemas": "json2ts -i src/schemas -o src/types",
    "build": "rm -rf dist && tsc",
    "test": "TEST=true vitest --ui",
    "dev": "DEV=true tsx watch src/index.ts",
    "start": "npm run build && PROD=true node dist/index.js",
    "cluster": "npm run build && PROD=true node dist/cluster.js",
    "autocannon": "autocannon -c 100 -d 30 -p 10 https://localhost:9000/",
    "artillery": "artillery run src/tests/load/loadtest.yml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/auth": "^4.6.0",
    "@fastify/autoload": "^5.8.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/oauth2": "^7.8.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@fastify/under-pressure": "^8.3.0",
    "@sinclair/typebox": "^0.32.14",
    "amqplib": "^0.10.3",
    "close-with-grace": "^1.3.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.1",
    "fastify-cookie": "^5.7.0",
    "fastify-plugin": "^4.5.1",
    "jsonwebtoken": "^9.0.2",
    "simple-get": "^4.0.1"
  },
  "devDependencies": {}
}
